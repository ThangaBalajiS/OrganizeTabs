{
    "name": "Infinite Tabs Manager",
    "version": "0.2.6",
    "description": "Makes your Browser faster and efficient",
    "permissions": ["tabs","activeTab","storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },    
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "assets/icon32.png"
        }
      },
    "icons": {
        "16": "assets/icon32.png",
        "32": "assets/icon32.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "manifest_version": 2

  }
